{
  "name": "TestForE2E",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest --runInBand",
    "lint": "eslint .",
    "start:appium": "appium --base-path /wd/hub",
    "test:e2e:all": "yarn test:e2e:android && yarn test:e2e:ios",
    "test:e2e:android": "E2E_DEVICE=\"android\" yarn test",
    "test:e2e:ios": "E2E_DEVICE=\"ios\" yarn test"
  },
  "dependencies": {
    "react": "18.2.0",
    "react-native": "0.71.4"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native-community/eslint-config": "^3.2.0",
    "@tsconfig/react-native": "^2.0.2",
    "@types/jest": "^29.2.1",
    "@types/react": "^18.0.24",
    "@types/react-test-renderer": "^18.0.0",
    "appium": "^2.0.0-beta.53",
    "appium-uiautomator2-driver": "^2.12.3",
    "appium-xcuitest-driver": "^4.16.12",
    "babel-jest": "^29.2.1",
    "eslint": "^8.19.0",
    "jest": "^29.2.1",
    "metro-react-native-babel-preset": "0.73.8",
    "prettier": "^2.8.4",
    "react-test-renderer": "18.2.0",
    "typescript": "4.9.5",
    "webdriverio": "^7.25.4"
  }
}
